<br />
<div class = "hero-unit" >
<div class="row">
  <div class="span6 offset3">

    <%=form_for(@company) do |c| %>
<h2>Edit Your Profile</h2>
<%=c.label :name %>
<%=c.text_field :name %>

<%=c.label :industry %>
<%=c.text_field :industry %>

<%=c.label :link, "Company Website" %>
<%=c.text_field :link %>

<%=c.label :password %>
<%=c.password_field :password %>
                        <br />
<%=c.submit "Save your changes", class: "btn btn-large btn-primary" %>
			<% end %>
			</div>
</div>
</div>


